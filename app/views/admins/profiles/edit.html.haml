.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admins_profiles_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_admins_profile_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admins_profile_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Perfil
    .inner
      = form_for @profile, :url => admins_profile_path(@profile), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
