.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admins_videos_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_admins_video_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Video
    .inner
      = form_for :video, :url => admins_videos_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
